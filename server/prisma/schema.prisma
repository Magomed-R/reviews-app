generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Review {
  id         String   @id @default(uuid())
  username   String
  text       String
  rating     Int      @default(5)
  image_url  String
  created_at DateTime @default(now())

  @@map("reviews")
}
